{
  "name": "codegen",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "apex",
    "codegen"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "templates",
    "dist",
    "README.md",
    "test",
    "src",
    "docs",
    "definitions",
    "templates"
  ],
  "eslintConfig": {
    "extends": "./config/.eslintrc.json"
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "shx rm -rf dist",
    "test": "npm run test:unit && npm run test:snapshot",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:snapshot": "cd testdata && ./diffcheck.sh",
    "style": "npm run format -- --list-different && npm run lint",
    "style:fix": "npm run format:fix && npm run lint:fix",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}\"",
    "format:fix": "npm run format -- --write",
    "lint": "eslint -c ./config/.eslintrc.json src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prerelease": "npm run build"
  },
  "dependencies": {
    "@apexlang/codegen": "file:../../apex-codegen",
    "@apexlang/core": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.22.0",
    "jest": "^29.1.1",
    "prettier": "^2.2.1",
    "shx": "0.3.3",
    "ts-jest": "^29.0.3",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "^4.8.0"
  }
}
